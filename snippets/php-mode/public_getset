public function get$1(){
	return !empty($this->${1:$(s-snake-case yas-text)}) ? $this->${1:$(s-snake-case yas-text)} : false;
}
public function set$1($val){
	$this->${1:$(s-snake-case yas-text)} = $val;
	return $this;
}
